						configd:delete "/opt/vyatta/share/tmplscripts/interfaces/all/ipv6/ospfv3/process/instance-id/cost/configd_delete.sh";
					}
					list network {
						max-elements "1";
						configd:help "Network type";
						configd:update "/opt/vyatta/share/tmplscripts/interfaces/all/ipv6/ospfv3/process/instance-id/network/configd_update.sh";
						configd:delete "/opt/vyatta/share/tmplscripts/interfaces/all/ipv6/ospfv3/process/instance-id/network/configd_delete.sh";
						key "tagnode";
						leaf tagnode {
							type enumeration {
								enum "broadcast" {
									configd:help "Broadcast network type";
								}
