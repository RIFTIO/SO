	grouping itf-ipv6-ospfv3 {
		container ospfv3 {
			configd:priority "948";
			configd:help "Set OSPFv3 instance parameters";
			leaf link-lsa-suppression {
				type string {
					configd:syntax "/opt/vyatta/share/tmplscripts/interfaces/all/ipv6/ospfv3/link-lsa-suppression/configd_syntax1.cli";
				}
				configd:help "Enable link-lsa-suppression for this instance-id";
				configd:allowed "/opt/vyatta/share/tmplscripts/interfaces/all/ipv6/ospfv3/link-lsa-suppression/configd_allowed.sh";
				configd:create "/opt/vyatta/share/tmplscripts/interfaces/all/ipv6/ospfv3/link-lsa-suppression/configd_create.sh";
				configd:delete "/opt/vyatta/share/tmplscripts/interfaces/all/ipv6/ospfv3/link-lsa-suppression/configd_delete.sh";
			}
			leaf priority {
				type uint32 {
					range 0..255;
				}
				configd:help "Router priority (default 1)";
				configd:update "/opt/vyatta/share/tmplscripts/interfaces/all/ipv6/ospfv3/priority/configd_update.sh";
				configd:delete "/opt/vyatta/share/tmplscripts/interfaces/all/ipv6/ospfv3/priority/configd_delete.sh";
			}
			list process {
				configd:help "IPv6 Open Shortest Path First (OSPFv3) Process ID";
				configd:allowed "/opt/vyatta/share/tmplscripts/interfaces/all/ipv6/ospfv3/process/configd_allowed.sh";
				key "tagnode";
				leaf tagnode {
					type string;
					configd:help "IPv6 Open Shortest Path First (OSPFv3) Process ID";
					configd:allowed "/opt/vyatta/share/tmplscripts/interfaces/all/ipv6/ospfv3/process/configd_allowed.sh";
				}
				list instance-id {
					max-elements "1";
					configd:help "Instance-id";
					key "tagnode";
					leaf tagnode {
						type uint32 {
							range 0..255;
						}
						configd:help "Instance-id";
					}
					leaf link-lsa-suppression {
						type string {
							configd:syntax "/opt/vyatta/share/tmplscripts/interfaces/all/ipv6/ospfv3/process/instance-id/link-lsa-suppression/configd_syntax1.cli";
						}
						configd:help "Enable link-lsa-suppression for this instance-id";
						configd:allowed "/opt/vyatta/share/tmplscripts/interfaces/all/ipv6/ospfv3/process/instance-id/link-lsa-suppression/configd_allowed.sh";
						configd:create "/opt/vyatta/share/tmplscripts/interfaces/all/ipv6/ospfv3/process/instance-id/link-lsa-suppression/configd_create.sh";
						configd:delete "/opt/vyatta/share/tmplscripts/interfaces/all/ipv6/ospfv3/process/instance-id/link-lsa-suppression/configd_delete.sh";
					}
					leaf priority {
						type uint32 {
							range 0..255;
						}
						configd:help "Router priority (default 1)";
						configd:update "/opt/vyatta/share/tmplscripts/interfaces/all/ipv6/ospfv3/process/instance-id/priority/configd_update.sh";
						configd:delete "/opt/vyatta/share/tmplscripts/interfaces/all/ipv6/ospfv3/process/instance-id/priority/configd_delete.sh";
					}
					list neighbor {
						configd:help "Configure ipv6 address for neighbor parameters";
						configd:update "/opt/vyatta/share/tmplscripts/interfaces/all/ipv6/ospfv3/process/instance-id/neighbor/configd_update.sh";
						configd:delete "/opt/vyatta/share/tmplscripts/interfaces/all/ipv6/ospfv3/process/instance-id/neighbor/configd_delete.sh";
						key "tagnode";
						leaf tagnode {
							type types:ipv6-address;
							configd:help "Configure ipv6 address for neighbor parameters";
						}
					}
					leaf dead-interval {
						type uint32 {
							range 1..65535;
						}
						configd:help "Interval after which neighbor is declared dead (default 40)";
						configd:update "/opt/vyatta/share/tmplscripts/interfaces/all/ipv6/ospfv3/process/instance-id/dead-interval/configd_update.sh";
						configd:delete "/opt/vyatta/share/tmplscripts/interfaces/all/ipv6/ospfv3/process/instance-id/dead-interval/configd_delete.sh";
					}
					leaf hello-interval {
						type uint32 {
							range 1..65535;
						}
						configd:help "Interval between hello packets (default 10)";
						configd:update "/opt/vyatta/share/tmplscripts/interfaces/all/ipv6/ospfv3/process/instance-id/hello-interval/configd_update.sh";
						configd:delete "/opt/vyatta/share/tmplscripts/interfaces/all/ipv6/ospfv3/process/instance-id/hello-interval/configd_delete.sh";
					}
					leaf retransmit-interval {
